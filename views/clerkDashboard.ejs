<div class="dashboard-container">
  <div class="admin-dashboard">
    <h1>Clerk Dashboard:</h1>
    <div class="dashboard-actions">
      <button onclick="location.href='/load-data/rentals'">Load Data</button>
      <button onclick="location.href='/rentals/new'">Add New Property</button>
    </div>
  </div>
  
  <div class="properties-container">
    <% rentals.forEach((rental) => { %>
      <div class="property-item">
        <img src="<%= rental.imageUrl %>" alt="Property Image">
        <div class="property-details">
          <h3><%= rental.headline %></h3>
          <!-- Include other details you want to show for each property -->
        </div>
        <div class="property-actions">
          <button onclick="location.href='/rentals/edit/<%= rental._id %>'">Edit</button>
          <button onclick="location.href='/rentals/delete/<%= rental._id %>'">Delete</button>
        </div>
      </div>
    <% }); %>
  </div>
</div>